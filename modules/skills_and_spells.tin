#ACTION {^  %s  %2  %s{$skillAndSpellLevels}}
{
	#script {i} {echo "%2" | sed -e 's/[^ ]*/\u&/g' -e 's/[^ .]/\l&/' | tr -d " "};
	#variable {i} {${i[1]}};
	#variable {$i} {true}
}
{9}

#VARIABLE         {skillAndSpellLevels}  {average|bad|exceptional|fair|good|moderate|perfect|poor|very bad|very good}
